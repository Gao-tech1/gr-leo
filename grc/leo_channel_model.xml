<?xml version="1.0"?>
<block>
  <name>LEO Channel</name>
  <key>leo_channel_model</key>
  <category>[LEO]</category>
  <import>import leo</import>
  <make>leo.leo_channel_model($sample_rate, $time_resolution_us, $tle_title, $tle_1, $tle_2, $gs_lat,
                              $gs_lon, $gs_alt, $obs_start, $obs_end)</make>
  
  <param>
    <name>Sampling Rate</name>
    <key>sample_rate</key>
    <type>float</type>
  </param>	
  
  <param>
    <name>Time resolution (us)</name>
    <key>time_resolution_us</key>
    <type>float</type>
  </param>	
  
  <param>
    <name>TLE Title</name>
    <key>tle_title</key>
    <type>string</type>
  </param>
  
  <param>
    <name>TLE 1</name>
    <key>tle_1</key>
    <type>string</type>
  </param>
	
  <param>
    <name>TLE 2</name>
    <key>tle_2</key>
    <type>string</type>
  </param>	
		 
  <param>
    <name>GS Latitude</name>
    <key>gs_lat</key>
    <type>float</type>
  </param>	
  
  <param>
    <name>GS Longtide</name>
    <key>gs_lon</key>
    <type>float</type>
  </param>
  
  <param>
    <name>GS Altitude</name>
    <key>gs_alt</key>
    <type>float</type>
  </param>	
  
  <param>
    <name>Observation Start</name>
    <key>obs_start</key>
    <type>string</type>
  </param>	
  
  <param>
    <name>Observation End</name>
    <key>obs_end</key>
    <type>string</type>
  </param>	
  
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>	
   
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  
</block>
