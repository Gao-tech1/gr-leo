<?xml version="1.0"?>
<block>
  <name>Tracker</name>
  <key>variable_tracker</key>
  <import>import leo</import>
  <var_make>self.$(id) = $(id) = leo.tracker_make($satellite_info, $gs_lat, $gs_lon, $gs_alt, $obs_start, $obs_end, $name)</var_make>
  <var_value>leo.tracker_make($satellite_info, $gs_lat, $gs_lon, $gs_alt, $obs_start, $obs_end, $name)</var_value>
  <make></make>
  <param>
      <name>Ignore Me</name>
      <key>value</key>
      <value>'fine'</value>
      <type>raw</type>
      <hide>all</hide>
  </param>
  
  <param>
    <name>Name</name>
    <key>name</key>
    <type>string</type>
    <hide>all</hide>
  </param>
  
  <param>
    <name>Satellite</name>
    <key>satellite_info</key>
    <value>satellite_variable</value>
    <type>raw</type>
  </param>

  <param>
    <name>Longtitude</name>
    <key>gs_lon</key>
    <type>float</type>
  </param>
  
  <param>
    <name>Latitude</name>
    <key>gs_lat</key>
    <type>float</type>
  </param>
  
  <param>
    <name>Altitude</name>
    <key>gs_alt</key>
    <type>float</type>
  </param>
  
  <param>
    <name>Observation Start Time</name>
    <key>obs_start</key>
    <type>string</type>
  </param>
  
   <param>
    <name>Observation End Time</name>
    <key>obs_end</key>
    <type>string</type>
  </param>

</block>
